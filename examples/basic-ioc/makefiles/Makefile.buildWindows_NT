	$(MAKE) clean
	cd ..
	cd sdr
	$(MAKE) clean
	cd ..
	cd ext-schema
	$(MAKE) clean
	cd ..
	cd dsm-gps
	$(MAKE) clean
	cd ..
	cd dsm-sdr
	$(MAKE) clean
	cd ..
