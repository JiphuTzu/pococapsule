	cd dsm-gps
	$(MAKE) all
	cd ..
	@echo
	@echo "*********** dsm-sdr example **********"
	cd dsm-sdr
	$(MAKE) all
	cd ..
	@echo
	@echo "*********** robotic-vehicle ***********"
	cd robotic-vehicle
	$(MAKE) all
	cd ..
	@echo
	@echo "*********** on-error *****************"
	cd on-error
	$(MAKE) all
	cd ..

